@model List<StackOverflowClone.ViewModels.QuestionViewModel>

@{
    ViewBag.Title = "HOME";
}

<div class="row">
    <div class="col-sm-12">

        <div class="container-fluid py-5 ">
            <h1 class="display-5 fw-bold">Learn Shared, Build</h1>
            <h3> Each month, over 50 million developers com to Stack Overflow to learn, share their knowledge, and build their careers</h3>
            <p class="lead"> Join the world's largest developer community.</p>
        </div>
    </div>

    <div class="row">
        <div class="col-sm-12">
            <table class="table">
                @foreach (var item in Model)
                {
                    Html.RenderPartial("Question", item);
                }
            </table>
            @if (Session["CurrentUserName"] != null)
            {
                <a href="/questions/create" class="btn btn-primary">Add question</a>
            }

        </div>

    </div>

</div>

